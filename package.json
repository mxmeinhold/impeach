        "pug": "^3.0.0",
        "shelljs": "^0.8.4"
    },
    "devDependencies": {
        "eslint": "^7.2.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "gulp": "^4.0.2",
        "gulp-clean-css": "^4.3.0",
        "gulp-eslint": "^6.0.0",
        "gulp-minify": "^3.1.0",
